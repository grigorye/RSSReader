"${GE_XCODE_SCRIPTS:?}"/VersionIsClean || exit 0

if [ "${CRASHLYTICS_ENABLED:?}" == "YES" ]
then
	./Fabric.framework/run "${XC_FABRIC_API_KEY:?}" "${XC_FABRIC_BUILD_SECRET:?}"
fi
