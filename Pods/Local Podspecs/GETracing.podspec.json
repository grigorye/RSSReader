{
  "name": "GETracing",
  "version": "0.1.1",
  "summary": "Seamless tracing for Swift.",
  "description": "Trace bar in `x = foo(bar)` as `x = foo(x$(bar)).`",
  "homepage": "https://github.com/grigorye/GETracing",
  "license": "MIT",
  "authors": {
    "Grigory Entin": "grigory.entin@gmail.com"
  },
  "platforms": {
    "ios": "10.0",
    "osx": "10.10"
  },
  "source": {
    "git": "https://github.com/grigorye/GETracing.git",
    "tag": "0.1.1"
  },
  "source_files": "GETracing/*.swift",
  "exclude_files": "GETracing/ModuleExports-*.swift",
  "preserve_paths": "Tools/*",
  "swift_version": "4.2",
  "xcconfig": {
    "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "GE_TRACE_ENABLED"
  },
  "user_target_xcconfig": {
    "GETRACING_TOOLS": "$(GETRACING_POD_ROOT)/Tools"
  },
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "source_files": "GETracingTests/**/*.swift",
      "resource_bundles": {
        "GETracingTests-Sources": [
          "GETracingTests"
        ]
      },
      "dependencies": {
        "CwlPreconditionTesting": [

        ],
        "CwlCatchException": [

        ]
      }
    }
  ]
}
